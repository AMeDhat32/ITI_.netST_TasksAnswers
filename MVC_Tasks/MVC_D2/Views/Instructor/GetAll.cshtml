<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">Salary</th>
        </tr>
    </thead>

    <tbody>

        @foreach (var Instructor in Model)
        {
            <tr>
                <th scope="row">@Instructor.Id</th>
                <td>@Instructor.Name</td>
                <td>@Instructor.Salary</td>
              
            </tr>
        }



    </tbody>
</table>