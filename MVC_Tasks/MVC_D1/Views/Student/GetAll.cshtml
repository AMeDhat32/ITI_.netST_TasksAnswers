<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">PhoneNumber</th>
        </tr>
    </thead>
    
    <tbody>
        
            @foreach (var Student in Model)
        {
            <tr>
                <th scope="row">@Student.SSN</th>
                <td>@Student.Name</td>
                <td>@Student.Age</td>
                <td>@Student.PhoneNumber</td>
            </tr>
            }

        
        
    </tbody>
</table>