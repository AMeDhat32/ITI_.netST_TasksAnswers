@model Student

<div class="card" style="width: 18rem;">
    <img class="card-img-top" src="~/pin3.jpg" alt="Card image cap">
    <div class="card-body">
        <h5 class="card-title">@Model.Name</h5>
        <p class="card-text">@Model.Address</p>
        <a href="#" class="btn btn-primary">@Model.Grade</a>
        <br />
        <br />
        <br />
        <a href="/Student/GetAll" class="btn btn-outline-success">Back to List</a>

    </div>
</div>
